<Window x:Class="LogicPuzzleGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LogicPuzzleGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Menu x:Name="menu" Height="19" VerticalAlignment="Top">
            <MenuItem Header="_File"/>
            <MenuItem Header="_Game">
                <MenuItem x:Name="StartNewMI" Header="Start _New" Click="StartNewMI_Click"/>
            </MenuItem>
        </Menu>
        <Grid Margin="0,24,0,0"  Name="MainGrid">
        </Grid>

    </Grid>
</Window>
